{
  "name": "sesh - Beautiful New Tab",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Make your New Tab pages beautiful, useful and minimal",
  "homepage_url": "https://fvrtrp.github.io/sesh",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
